<?xml version="1.0"?>

<launch>
  <arg name="serial_port" default="/dev/ttyACM0" />
  <arg name="baud" default="230400" />

  <arg name="debug" default="True" />

  <!-- f9p receiver -->
  <node pkg="f9p_ichimill" type="f9p_driver.py" name="f9p_driver" args="" >

    <param name="port" value="$(arg serial_port)"/>
    <param name="baud" value="$(arg baud)"/>

    <param name="debug" value="$(arg debug)"/>
  </node>

  <arg name="username" default="" />
  <arg name="password" default="" />
  <arg name="port" default="2101" />
  <arg name="host" default="ntrip.ales-corp.co.jp" />
  <arg name="mountpoint" default="32M7NHS" />

  <!-- ichimill -->
  <node pkg="f9p_ichimill" type="ichimill_connect.py" name="ichimill_connect" args="" output="screen" >

    <param name="username" value="$(arg username)"/>
    <param name="password" value="$(arg password)"/>
    <param name="port" value="$(arg port)"/>

    <param name="host" value="$(arg host)"/>
    <param name="mountpoint" value="$(arg mountpoint)"/>

    <param name="debug" value="$(arg debug)"/>
  </node>

  <!-- sentence to fix -->
  <node pkg="nmea_navsat_driver" type="nmea_topic_driver" name="nmea_topic_driver" >
  </node>
</launch>
